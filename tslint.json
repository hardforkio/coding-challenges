{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["./node_modules"]
  },
  "rules": {
    "interface-name": [true, "never-prefix"],
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "arrow-parens": true,
    "semicolon": [true, "never"]
  }
}
